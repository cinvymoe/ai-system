            "zustand": "^5.0.9"
      },
      "devDependencies": {
            "@types/node": "^20.19.25",
            "@vitejs/plugin-react-swc": "^3.10.2",
            "concurrently": "^9.2.1",
            "cross-env": "^10.1.0",
            "electron": "^39.2.3",
            "electron-builder": "^26.0.12",
            "typescript": "^5.9.3",
            "vite": "6.3.5",
            "wait-on": "^9.0.3"
      },
      "scripts": {
            "dev": "concurrently \"npm run dev:vite\" \"npm run dev:electron\"",
            "dev:vite": "vite",
            "dev:electron": "wait-on http://localhost:3000 && cross-env NODE_ENV=development electron . --no-sandbox",
            "dev:debug": "wait-on http://localhost:3000 && cross-env NODE_ENV=development electron . --no-sandbox --enable-logging --log-level=0",
            "build": "npm run build:vite && npm run build:electron",
            "build:vite": "cross-env ELECTRON=true vite build",
            "build:electron": "tsc -p electron/tsconfig.json",
            "pack": "npm run build && electron-builder --dir",
            "pack:linux": "npm run build && electron-builder --linux --dir",
            "pack:arm64": "npm run build && electron-builder --linux --arm64 --dir",
            "dist": "npm run build && electron-builder",
            "dist:arm64": "npm run build && electron-builder --linux --arm64",
            "dist:linux": "npm run build && electron-builder --linux",
            "start:debug": "electron . --enable-logging --log-level=0 --no-sandbox"
      }
}
